<header class="container-fluid">
  <div class="row-fluid">
    <h1 class="span10">General Assembly</h1>
      <% if user_signed_in? %>
        <%= link_to "HELLO, #{current_user.first_name}", edit_user_path(current_user) %>
        <%= link_to "Not you?", destroy_user_session_path, method: :delete %>
        
      <% else %>
        <%= link_to "Login", new_user_session_path, :class=>'span2' %>
      <% end %>
  </div>
</header>